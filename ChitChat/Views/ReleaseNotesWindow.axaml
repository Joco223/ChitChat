<Window xmlns="https://github.com/avaloniaui"
		x:Class="ChitChat.Views.ReleaseNotesWindow"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:vm="using:ChitChat.ViewModels"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:ChitChat.Views"
		mc:Ignorable="d"
		Title="ReleaseNotesWindow" Height="600" Width="600"
		Loaded="Window_Loaded">
	<Grid  Margin="10">
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<Label x:Name="titleLabel" Grid.Row="0" Content="Release notes" FontWeight="Bold" FontSize="30" HorizontalAlignment="Center" />
		<Border Grid.Row="1" BorderBrush="Gray" BorderThickness="2" Margin="5">
			<ScrollViewer x:Name="notesScrollViewer" Padding="5">
				<!--<StackPanel x:Name="notesStackPanel" HorizontalAlignment="Stretch" Margin="5">
				</StackPanel>-->
			</ScrollViewer>
		</Border>
	</Grid>
</Window>
